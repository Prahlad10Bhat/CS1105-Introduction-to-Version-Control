# Switch to development branch
git checkout development

# Create scripts directory and files
mkdir scripts
echo 'echo "Setup script executed"' > scripts/setup.sh
echo 'echo "Deploy script executed"' > scripts/deploy.sh
echo 'echo "Test script executed"' > scripts/test.sh

# Add and commit the scripts
git add scripts/
git commit -m "Added scripts folder with setup, deploy, and test scripts"
